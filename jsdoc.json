{  "source": {
    "include": ["src"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },  "opts": {
    "destination": "./docs/jsdoc",
    "recurse": true,
    "readme": "./README.md",
    "template": "node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "homepageTitle": "Matter.js + Babylon.js Integration",
      "theme": "dark",
      "title": "Matter.js + Babylon.js Integration",
      "menu": [
        {
          "title": "GitHub",
          "link": "#",
          "target": "_blank"
        }
      ],
      "meta": [
        {
          "name": "author",
          "content": "Project Team"
        },
        {
          "name": "description",
          "content": "Integration of Matter.js and Babylon.js"
        }
      ]
    }
  }
}
